{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "jsx-double"],
        "semicolon": [true, "always", "ignore-interfaces"],
        "object-literal-sort-keys": false,
        "interface-name": false,
        "member-access": false,
        "no-empty-interface": false
    },
    "rulesDirectory": []
}